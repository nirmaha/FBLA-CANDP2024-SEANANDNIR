<Page x:Class="EduPartners.MVVM.View.Pages.Notifications"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:EduPartners.MVVM.View.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Notifications">

    <Border CornerRadius="5" Background="{DynamicResource SecundaryBackgroundColor}">
        <TextBlock Text="Notifications" FontSize="30" Foreground="{DynamicResource PrimaryTextColor}" FontWeight="Bold"
                VerticalAlignment="Center" HorizontalAlignment="Center"/>
    </Border>
</Page>
